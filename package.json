{
  "name": "svelte-markdown",
  "description": "A markdown renderer for Svelte",
  "version": "0.5.0",
  "packageManager": "yarn@4.4.0",
  "engines": {
    "node": ">=20"
  },
  "main": "dist/sveltemarkdown.js",
  "module": "dist/sveltemarkdown.es.js",
  "jsnext:main": "dist/sveltemarkdown.es.js",
  "svelte": "src/index.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "svelte": "./src/index.js"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "files": [
    "dist",
    "src",
    "types"
  ],
  "license": "MIT",
  "author": "Pablo Berganza <pablo@berganza.dev>",
  "repository": "github:pablo-abc/svelte-markdown",
  "homepage": "https://github.com/pablo-abc/svelte-markdown",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "2.5.3",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/svelte": "4.2.3",
    "eslint": "7.32.0",
    "eslint-plugin-svelte": "2.43.0",
    "jsdom": "22.1.0",
    "prettier": "2.8.8",
    "rollup-plugin-analyzer": "4.0.0",
    "rollup-plugin-filesize": "10.0.0",
    "rollup-plugin-terser": "7.0.2",
    "sirv-cli": "1.0.14",
    "svelte": "4.2.19",
    "svelte-eslint-parser": "0.32.2",
    "vite": "4.5.3",
    "vitest": "0.33.0"
  },
  "dependencies": {
    "@types/marked": "5.0.2",
    "marked": "7.0.5"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "types": "./types/index.d.ts"
}
